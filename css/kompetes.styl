.vote_picture
  padding 0


  label
    display: block;
    position: relative;
    cursor: pointer;


  label:before
    background-color: white;
    color: white;
    content: " ";
    display: block;
    border: 1px solid #00a65a;
    position: absolute;
    font-size: 20px;
    top: 40%;
    left: 30%;
    width: 150px;
    height: 45px;
    text-align: center;
    line-height: 28px;
    transition-duration: 0.4s;
    transform: scale(0);
    padding-top: 10px





  input[type="checkbox"][id^="cb"]
    display: none;

  :checked + label
    /*border-color: #ddd;*/


  :hover + label:before
    content:  "Vote";
    background-color: black;
    opacity: 0.6;
    transform: scale(1);
    z-index: 999999999;
    margin-top: 50px;
    margin-left: 50px;
    border-radius: 20px;


  :checked + label:before
    content: "Voted ✓";
    background-color: black;
    opacity: 0.6;
    transform: scale(1);
    z-index: 99999;
    margin-top: 50px;
    margin-left: 50px;
    border-radius: 20px;

  :checked + label img
    /*transform: scale(0.9);*/
    /*box-shadow: 0 0 5px #333;*/
    z-index: -1;



