/* checkbox for uploading photo category */

input.check { position: fixed; left:-100px;}
input.check + label[for].check {
  cursor: pointer;
  color: #999;
  font-size: 16px;
  //font-family: Arial;
  border: 1px solid #CCC;
  padding: 5px;
  font-family: 'Droid Sans',sans-serif
}
input.check + label[for].check:after {
  content: "";
  display: inline-block;
  width: 35px;
  height: 15px;
  vertical-align: middle;
  text-align: right;
  margin: 0 5px 0 10px;
  line-height: 0.7em;
  background-position: left;
}

/* Checked */
input:checked + label[for].check:after {
  color: #f00;
  content:"✔";
}

/* Focus */
input:focus + label[for].check {
  border-color: #6FABE7;
}

/* Hover */
input + label[for].check:hover {
  border-color: #6FABE7;
}

/* Hover (unchecked) */
input:not(:checked) + label[for].check:hover:after {
  color: #DDD;
  content:"✔";
}

/* Hover (checked) */
input:checked + label[for].check:hover:after {
  color: #6FABE7;
  content:"✔";
}


/* Switch button */


.toggleWrapper {
  position: absolute;
  top: 50%;
  left: 50%;
  overflow: hidden;
  -webkit-transform: translate3d(-50%, -50%, 0);
  transform: translate3d(-50%, -50%, 0);
}
.toggleWrapper input {
  position: absolute;
  left: -99em;
}

.toggle {
  cursor: pointer;
  display: inline-block;
  position: relative;
  margin-bottom: 4px
  width: 120px;
  height: 30px;
  background: #D21626;
  border-radius: 5px;
  -webkit-transition: all 200ms cubic-bezier(0.445, 0.05, 0.55, 0.95);
  transition: all 200ms cubic-bezier(0.445, 0.05, 0.55, 0.95);
}
.toggle:before, .toggle:after {
  position: absolute;
  line-height: 30px;
  font-size: 14px;
  z-index: 2;
  -webkit-transition: all 200ms cubic-bezier(0.445, 0.05, 0.55, 0.95);
  transition: all 200ms cubic-bezier(0.445, 0.05, 0.55, 0.95);
}
.toggle:before {
  content: "OFF";
  left: 20px;
  color: #D21626;
}
.toggle:after {
  content: "ON";
  right: 20px;
  color: #fff;
}

.toggle__handler {
  display: inline-block;
  position: relative;
  z-index: 1;
  background: #fff;
  width: 65px;
  height: 25px;
  border-radius: 3px;
  top: 3px;
  left: 3px;
  -webkit-transition: all 200ms cubic-bezier(0.445, 0.05, 0.55, 0.95);
  transition: all 200ms cubic-bezier(0.445, 0.05, 0.55, 0.95);
  -webkit-transform: translateX(0px);
  transform: translateX(0px);
}

input:checked + .toggle {
  background: #66B317;
}
input:checked + .toggle:before {
  color: #fff;
}
input:checked + .toggle:after {
  color: #66B317;
}
input:checked + .toggle .toggle__handler {
  width: 54px;
  -webkit-transform: translateX(60px);
  transform: translateX(60px);
  border-color: #fff;
}







